    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: 当前文件",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "./groundtruth.json",
                "./predict.json"
            ]
        },
        {
            "name": "main debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/DipFantasy", //启动的程序
            "args": [
                "inputs/class/2.png" //输入的图像
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "internalConsoleOptions": "neverOpen",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "main_debug"
        },
        {
            "name": "module debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/lib/debug/DipFantasy_Debug", //启动的程序
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "internalConsoleOptions": "neverOpen",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "module_debug"
        }
    ]
}